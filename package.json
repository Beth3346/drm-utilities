    "gulp-sourcemaps": "2.6.4",
    "gulp-util": "3.0.8",
    "husky": "^1.3.1",
    "jshint": "2.10.1",
    "mocha": "^5.2.0",
    "mocha-notifier-reporter": "^0.1.2",
