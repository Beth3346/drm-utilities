{
  "name": "elr-utilities",
  "version": "1.0.0",
  "description": "a demonstration of what elr-utilities can do",
  "main": "js/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/elr-utilities/elr-utilities.git"
  },
  "scripts": {
    "build": "build",
    "test": "nyc mocha test/test.js --compilers js:babel-core/register",
    "test watch": "nyc mocha test/test.js watch --compilers js:babel-core/register",
    "cover": "nyc --reporter=lcov npm test",
    "report-coverage": "cat ./coverage/lcov.info | codecov"
  },
  "dependencies": {
    "babel-runtime": "6.20.0",
    "elr-accordion": "2.1.1",
    "elr-accordion-nav": "2.0.2",
    "elr-alerts": "1.2.0",
    "elr-back-to-top": "1.1.0",
    "elr-dropdown-buttons": "1.0.3",
    "elr-dropdown-menu": "1.0.3",
    "elr-filter-grid": "1.1.0",
    "elr-lightbox": "1.1.0",
    "elr-modal": "1.1.0",
    "elr-modal-alerts": "1.1.0",
    "elr-offscreen-menus": "1.0.2",
    "elr-password": "1.2.0",
    "elr-popovers": "1.0.3",
    "elr-sass-utilities": "3.0.7",
    "elr-simple-slider": "1.1.1",
    "elr-sort": "1.1.0",
    "elr-sticky-nav": "1.1.0",
    "elr-tablefilter": "1.1.0",
    "elr-tablesorter": "1.1.2",
    "elr-tabs": "1.0.4",
    "elr-time-utilities": "1.2.1",
    "elr-ui": "0.1.2",
    "elr-utility-lib": "2.0.1",
    "jquery": "3.1.1"
  },
  "devDependencies": {
    "babel-core": "6.21.0",
    "babel-preset-es2015": "6.18.0",
    "browser-sync": "2.18.5",
    "del": "2.2.2",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "3.1.1",
    "gulp-babel": "6.1.2",
    "gulp-clean-css": "2.3.2",
    "gulp-csslint": "1.0.0",
    "gulp-eslint": "3.0.1",
    "gulp-imagemin": "3.1.1",
    "gulp-jshint": "2.0.4",
    "gulp-plumber": "1.1.0",
    "gulp-pug": "3.2.0",
    "gulp-sass": "3.0.0",
    "gulp-scss-lint": "0.4.0",
    "gulp-sourcemaps": "2.1.1",
    "gulp-util": "3.0.7",
    "jshint": "2.9.4",
    "webpack": "1.14.0",
    "webpack-stream": "3.2.0",
    "mocha": "^3.2.0",
    "mocha-notifier-reporter": "^0.1.2",
    "nyc": "^10.0.0",
    "chai": "^3.5.0",
    "chai-arrays": "0.0.1",
    "chai-jquery": "^2.0.0",
    "chai-shallow-deep-equal": "^1.4.4",
    "chai-subset": "^1.3.0",
    "codecov": "^1.0.1",
    "ghooks": "^2.0.0",
    "gulp-mocha": "^3.0.1",
    "gulp-notify": "^2.2.0"
  },
  "config": {
    "ghooks": {
      "pre-commit": "gulp"
    }
  },
  "nyc": {
    "check-coverage": true,
    "branches": 0,
    "functions": 0,
    "lines": 0,
    "statements": 0,
    "include": [
      "js/assets/*.js"
    ]
