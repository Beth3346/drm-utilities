section#pagination
    .elr-container
        .elr-row
            .elr-col-full
                h3.elr-bold Pagination
        .elr-row
            .elr-col-full.example-holder.example-holder-light
                nav.pagination.pagination-base
                    ul
                        li: a(href="#" aria-label="Previous") <i class="fa fa-angle-double-left"></i></i>
                        li: a(href="#") 1
                        li: a.active(href="#") 2
                        li: a(href="#") 3
                        li: a(href="#") 4
                        li: a(href="#") 5
                        li: a.disabled(href="#" aria-label="Next") <i class="fa fa-angle-double-right"></i>
