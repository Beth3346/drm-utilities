    "elr-dropdown-menu": "1.0.3",
    "elr-filter-grid": "1.1.2",
    "elr-lightbox": "1.2.0",
    "elr-modal": "1.1.2",
    "elr-modal-alerts": "1.1.0",
    "elr-offscreen-menus": "1.0.2",
    "elr-password": "1.2.0",
    "elr-popovers": "1.0.3",
    "elr-sass-utilities": "3.3.0",
