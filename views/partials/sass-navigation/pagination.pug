section#pagination
    .elr-container
        .elr-row
            .elr-col-full
                h3.example-heading Pagination
            .elr-col-full.example-holder
                nav.pagination.pagination-links-round
                    a.previous-pager(href="#" aria-label="Previous") <i class="fa fa-angle-double-left"></i> Previous
                    ul.pagination-list
                        li: a(href="#" aria-label="Previous") <i class="fa fa-angle-double-left"></i></i>
                        li: a(href="#") 1
                        li: a.active(href="#") 2
                        li: a(href="#") 3
                        li: a(href="#") 4
                        li: a(href="#") 5
                        li: a.disabled(href="#" aria-label="Next") <i class="fa fa-angle-double-right"></i>
                    a.next-pager.disabled(href="#" aria-label="Next") Next <i class="fa fa-angle-double-right"></i>